<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/numberBody"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="14dp"
    android:background="#f8f8f8" >
	
    <RelativeLayout
        android:id="@+id/headerLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">
        
        <TextView
		    android:id="@+id/headerTitleTextView"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:gravity="center"
		    android:textColor="#000000"
		    android:text="Week Of: April 10 - April 16"/>
	    
		<ImageButton
		    android:id="@+id/videoButton"
		    android:layout_width="30dp"
		    android:layout_height="21dp"
		    android:layout_toEndOf="@+id/headerTitleTextView"
		    android:layout_marginStart="14dp"
		    android:layout_centerVertical="true"
		    android:background="@drawable/vdo_btn"/>    
        
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/feedsLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/headerLayout" 
        android:layout_marginTop="14dp"
        android:orientation="vertical">
    </LinearLayout>
</RelativeLayout>
